<Project>
	<PropertyGroup>
		<ManagePackageVersionsCentrally>true</ManagePackageVersionsCentrally>
	</PropertyGroup>
	<ItemGroup>
		<PackageVersion Include="Microsoft.Extensions.DependencyInjection" Version="10.0.0"/>
		<PackageVersion Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="10.0.0"/>
		<PackageVersion Include="System.CommandLine" Version="2.0.0"/>
		<!--
			Analyzers
		-->
		<PackageVersion Include="Microsoft.VisualStudio.Threading.Analyzers" Version="17.14.15"/>
		<PackageVersion Include="SonarAnalyzer.CSharp" Version="10.15.0.120848"/>
		<PackageVersion Include="StyleCop.Analyzers.Unstable" Version="1.2.0.556"/>
		<!--
			Build
		-->
		<PackageVersion Include="Nerdbank.GitVersioning" Version="3.9.50"/>
		<PackageVersion Include="Nuke.Common" Version="9.0.4"/>
		<!--
			Generators
		-->
		<PackageVersion Include="Microsoft.CodeAnalysis.Analyzers" Version="3.11.0"/>
		<!-- Deliberately kept at a lower version, so we can support older compiler versions -->
		<PackageVersion Include="Microsoft.CodeAnalysis.CSharp" Version="4.10.0"/>
		<!--
			Testing
			Note that Rider doesn't properly support MSTest v4 yet, see:
			https://youtrack.jetbrains.com/issue/RSRP-502018/Rider-ReSharper-test-runner-runs-only-first-DataRow-with-MSTest-4
		-->
		<PackageVersion Include="Microsoft.NET.Test.Sdk" Version="17.14.1"/>
		<PackageVersion Include="Moq" Version="4.20.72"/>
		<PackageVersion Include="MSTest.TestAdapter" Version="3.11.0"/>
		<PackageVersion Include="MSTest.TestFramework" Version="3.11.0"/>
		<PackageVersion Include="Verify.MSTest" Version="30.20.1"/>
	</ItemGroup>
</Project>